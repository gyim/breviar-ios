date
chmod u=rwx *.*
make
cp uncgi uncgi.cgi
g++ breviar.cpp dbzaltar.cpp liturgia.cpp mygetopt.cpp mylog.cpp myhpage.cpp myexpt.cpp cgiutils.cpp myconf.cpp mydefs.cpp mystring.cpp
mv a.out l.cgi
chmod u=rw *.c
chmod u=rw *.cpp
chmod u=rw *.h
chmod oa+x *.cgi
gzip -c -v l.cgi > l.cgi.gz
gzip -c -v uncgi.cgi > uncgi.cgi.gz
gzip -c -v uncgi > uncgi.gz
date

